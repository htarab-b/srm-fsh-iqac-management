{
  "builds": [
    {
      "src": "iqac/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "iqac/wsgi.py"
    }
  ],
  "scripts": {
    "build": "python manage.py collectstatic --noinput"
  }
}